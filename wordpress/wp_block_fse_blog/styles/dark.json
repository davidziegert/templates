{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 3,
  "title": "Dark",
  "settings": {
    "appearanceTools": true,
    "color": {
      "defaultDuotone": false,
      "defaultPalette": false,
      "defaultGradients": false,
      "palette": [
        {
          "color": "#212121",
          "name": "Background",
          "slug": "background"
        },
        {
          "color": "#ededed",
          "name": "Foreground",
          "slug": "foreground"
        },
        {
          "color": "#ec5367",
          "name": "Primary",
          "slug": "primary"
        },
        {
          "color": "#f1818b",
          "name": "Secondary",
          "slug": "secondary"
        },
        {
          "color": "#3b3b3b",
          "name": "Shadow-1",
          "slug": "shadow-1"
        },
        {
          "color": "#6e6e6e",
          "name": "Shadow-2",
          "slug": "shadow-2"
        }
      ]
    },
    "layout": {
      "contentSize": "620px",
      "wideSize": "1280px"
    },
    "spacing": {
      "spacingScale": {
        "steps": 1
      },
      "spacingSizes": [
        {
          "name": "1",
          "size": "1rem",
          "slug": "10"
        }
      ],
      "units": ["%", "px", "em", "rem", "vh", "vw"]
    },
    "typography": {
      "fluid": false,
      "fontFamilies": [
        {
          "fontFamily": "serif",
          "slug": "serif",
          "name": "Serif"
        },
        {
          "fontFamily": "sans-serif",
          "slug": "sans-serif",
          "name": "Sans-Serif"
        },
        {
          "fontFamily": "monospace",
          "slug": "monospace",
          "name": "Monospace"
        },
        {
          "fontFamily": "system-ui",
          "slug": "system-ui",
          "name": "System-UI"
        }
      ],
      "writingMode": true
    },
    "useRootPaddingAwareAlignments": true
  },
  "templateParts": [
    {
      "area": "header",
      "name": "header",
      "title": "Header"
    },
    {
      "area": "footer",
      "name": "footer",
      "title": "Footer"
    },
    {
      "area": "comments",
      "name": "comments",
      "title": "comments"
    },
    {
      "area": "searchbar",
      "name": "searchbar",
      "title": "Searchbar"
    }
  ],
  "styles": {
    "blocks": {
      "core/avatar": {
        "border": {
          "radius": "80px"
        }
      },
      "core/button": {
        "variations": {
          "outline": {
            "spacing": {
              "padding": {
                "bottom": "0.5rem",
                "left": "1rem",
                "right": "1rem",
                "top": "0.5rem"
              }
            },
            "border": {
              "radius": ".33rem",
              "color": "var(--wp--preset--color--shadow-2)"
            }
          }
        }
      },
      "core/buttons": {
        "spacing": {
          "blockGap": "1rem"
        }
      },
      "core/calendar": {
        "color": {
          "text": "var(--wp--preset--color--secondary)"
        }
      },
      "core/categories": {},
      "core/code": {
        "border": {
          "color": "var(--wp--preset--color--shadow-1)",
          "radius": "var(--wp--preset--spacing--10)"
        },
        "color": {
          "background": "var(--wp--preset--color--shadow-2)",
          "text": "var(--wp--preset--color--secondary)"
        },
        "spacing": {
          "padding": {
            "bottom": "1.0rem",
            "left": "1.0rem",
            "right": "1.0rem",
            "top": "1.0rem"
          }
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--monospace)",
          "fontStyle": "normal",
          "fontWeight": "400",
          "lineHeight": "1.4"
        }
      },
      "core/comment-author-name": {
        "color": {
          "text": "var(--wp--preset--color--secondary)"
        },
        "elements": {
          "link": {
            "color": {
              "text": "var(--wp--preset--color--secondary)"
            },
            "typography": {
              "textDecoration": "none"
            },
            ":hover": {
              "typography": {
                "textDecoration": "underline"
              }
            }
          }
        },
        "typography": {
          "fontSize": "0.83rem",
          "fontStyle": "normal",
          "fontWeight": "600"
        }
      },
      "core/comment-content": {
        "typography": {
          "fontSize": "0.83rem"
        },
        "spacing": {
          "margin": {
            "top": "var(--wp--preset--spacing--10)",
            "bottom": "var(--wp--preset--spacing--10)"
          }
        }
      },
      "core/comment-date": {
        "color": {
          "text": "var(--wp--preset--color--secondary)"
        },
        "elements": {
          "link": {
            "color": {
              "text": "var(--wp--preset--color--secondary)"
            },
            "typography": {
              "textDecoration": "none"
            },
            ":hover": {
              "typography": {
                "textDecoration": "underline"
              }
            }
          }
        },
        "typography": {
          "fontSize": "0.83rem"
        },
        "spacing": {
          "margin": {
            "top": "0px",
            "bottom": "0px"
          }
        }
      },
      "core/comment-edit-link": {
        "elements": {
          "link": {
            "color": {
              "text": "var(--wp--preset--color--secondary)"
            },
            "typography": {
              "textDecoration": "none"
            },
            ":hover": {
              "typography": {
                "textDecoration": "underline"
              }
            }
          }
        },
        "typography": {
          "fontSize": "0.83rem"
        }
      },
      "core/comment-reply-link": {
        "elements": {
          "link": {
            "color": {
              "text": "var(--wp--preset--color--secondary)"
            },
            "typography": {
              "textDecoration": "none"
            },
            ":hover": {
              "typography": {
                "textDecoration": "underline"
              }
            }
          }
        },
        "typography": {
          "fontSize": "0.83rem"
        }
      },
      "core/post-comments-form": {
        "css": "& textarea, input{border-radius:.33rem}"
      },
      "core/comments-pagination": {
        "typography": {
          "fontSize": "0.83rem"
        }
      },
      "core/comments-pagination-next": {
        "typography": {
          "fontSize": "0.83rem"
        }
      },
      "core/comments-pagination-numbers": {
        "typography": {
          "fontSize": "0.83rem"
        }
      },
      "core/comments-pagination-previous": {
        "typography": {
          "fontSize": "0.83rem"
        }
      },
      "core/footnotes": {
        "typography": {
          "fontSize": "0.83rem"
        }
      },
      "core/gallery": {
        "spacing": {
          "margin": {
            "bottom": "var(--wp--preset--spacing--10)"
          }
        }
      },
      "core/image": {
        "variations": {
          "rounded": {
            "border": {
              "radius": "var(--wp--preset--spacing--10)"
            }
          }
        }
      },
      "core/list": {
        "spacing": {
          "padding": {
            "left": "var(--wp--preset--spacing--10)"
          }
        }
      },
      "core/loginout": {},
      "core/navigation": {
        "elements": {
          "link": {
            ":hover": {
              "typography": {
                "textDecoration": "underline"
              }
            },
            "typography": {
              "textDecoration": "none"
            }
          }
        },
        "typography": {
          "fontWeight": "500"
        }
      },
      "core/post-author": {
        "typography": {
          "fontSize": "0.83rem"
        }
      },
      "core/post-author-name": {
        "color": {
          "text": "var(--wp--preset--color--secondary)"
        },
        "elements": {
          "link": {
            ":hover": {
              "typography": {
                "textDecoration": "underline"
              }
            },
            "color": {
              "text": "var(--wp--preset--color--secondary)"
            },
            "typography": {
              "textDecoration": "none"
            }
          }
        },
        "typography": {
          "fontSize": "0.83rem"
        }
      },
      "core/post-date": {
        "color": {
          "text": "var(--wp--preset--color--secondary)"
        },
        "elements": {
          "link": {
            ":hover": {
              "typography": {
                "textDecoration": "underline"
              }
            },
            "color": {
              "text": "var(--wp--preset--color--secondary)"
            },
            "typography": {
              "textDecoration": "none"
            }
          }
        },
        "typography": {
          "fontSize": "0.83rem"
        }
      },
      "core/post-excerpt": {
        "typography": {
          "lineHeight": "1.4"
        }
      },
      "core/post-featured-image": {
        "border": {
          "radius": "var(--wp--preset--spacing--10)"
        }
      },
      "core/post-terms": {
        "elements": {
          "link": {
            ":hover": {
              "typography": {
                "textDecoration": "underline"
              }
            },
            "typography": {
              "textDecoration": "none"
            }
          }
        },
        "typography": {
          "fontSize": "0.83rem"
        }
      },
      "core/post-title": {
        "elements": {
          "link": {
            ":hover": {
              "typography": {
                "textDecoration": "underline"
              }
            },
            "typography": {
              "textDecoration": "none"
            }
          }
        }
      },
      "core/pullquote": {
        "border": {
          "radius": "var(--wp--preset--spacing--10)"
        },
        "elements": {
          "cite": {
            "typography": {
              "fontFamily": "var(--wp--preset--font-family--system-ui)",
              "fontSize": "0.83rem",
              "fontStyle": "normal"
            }
          }
        },
        "spacing": {
          "padding": {
            "bottom": "var(--wp--preset--spacing--10)",
            "top": "var(--wp--preset--spacing--10)"
          }
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--system-ui)",
          "fontSize": "1.0rem",
          "fontStyle": "italic",
          "fontWeight": "400",
          "letterSpacing": "0em",
          "lineHeight": "1.4"
        }
      },
      "core/query-title": {},
      "core/query-no-results": {
        "spacing": {
          "padding": {
            "top": "var(--wp--preset--spacing--10)"
          }
        }
      },
      "core/quote": {
        "border": {
          "radius": "var(--wp--preset--spacing--10)"
        },
        "color": {
          "background": "var(--wp--preset--color--secondary)"
        },
        "elements": {
          "cite": {
            "typography": {
              "fontFamily": "var(--wp--preset--font-family--system-ui)",
              "fontSize": "0.83rem",
              "fontStyle": "normal"
            }
          }
        },
        "spacing": {
          "padding": {
            "bottom": "1.0rem",
            "left": "1.0rem",
            "right": "1.0rem",
            "top": "1.0rem"
          }
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--system-ui)",
          "fontSize": "1.125rem",
          "fontStyle": "italic",
          "lineHeight": "1.4"
        },
        "variations": {
          "plain": {
            "border": {
              "color": "var(--wp--preset--color--secondary)",
              "radius": "0",
              "style": "solid",
              "width": "0"
            },
            "color": {
              "background": "transparent"
            },
            "spacing": {
              "padding": {
                "bottom": "1.0rem",
                "left": "1.0rem",
                "right": "1.0rem",
                "top": "1.0rem"
              }
            },
            "typography": {
              "fontFamily": "var(--wp--preset--font-family--system-ui)",
              "fontStyle": "normal",
              "fontSize": "1.125rem",
              "lineHeight": "1.4"
            }
          }
        }
      },
      "core/search": {
        "typography": {
          "fontSize": "0.83rem"
        },
        "elements": {
          "button": {
            "border": {
              "radius": { "ref": "styles.elements.button.border.radius" }
            }
          }
        }
      },
      "core/separator": {
        "border": {
          "color": "currentColor",
          "style": "solid",
          "width": "0 0 1px 0"
        },
        "color": {
          "text": "var(--wp--preset--color--shadow-1)"
        }
      },
      "core/site-tagline": {
        "elements": {
          "link": {
            ":hover": {
              "typography": {
                "textDecoration": "none"
              }
            },
            "typography": {
              "textDecoration": "none"
            }
          }
        },
        "color": {
          "text": "var(--wp--preset--color--primary)"
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--system-ui)",
          "fontSize": "1.25rem",
          "fontStyle": "italic",
          "fontWeight": "400"
        }
      },
      "core/site-title": {
        "elements": {
          "link": {
            ":hover": {
              "typography": {
                "textDecoration": "none"
              }
            },
            "typography": {
              "textDecoration": "none"
            }
          }
        },
        "color": {
          "text": "var(--wp--preset--color--primary)"
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--system-ui)",
          "fontSize": "1.25rem",
          "fontStyle": "normal",
          "fontWeight": "600"
        }
      }
    },
    "color": {
      "background": "var(--wp--preset--color--background)",
      "text": "var(--wp--preset--color--foreground)"
    },
    "elements": {
      "button": {
        ":active": {
          "color": {
            "background": "var(--wp--preset--color--primary)",
            "text": "var(--wp--preset--color--background)"
          }
        },
        ":focus": {
          "color": {
            "background": "var(--wp--preset--color--primary)",
            "text": "var(--wp--preset--color--background)"
          },
          "outline": {},
          "border": {
            "color": "var(--wp--preset--color--shadow-2)"
          }
        },
        ":hover": {
          "color": {
            "background": "var(--wp--preset--color--secondary)",
            "text": "var(--wp--preset--color--background)"
          },
          "border": {
            "color": "var(--wp--preset--color--shadow-2)"
          }
        },
        "border": {
          "radius": ".33rem",
          "color": "var(--wp--preset--color--shadow-2)"
        },
        "color": {
          "background": "var(--wp--preset--color--primary)",
          "text": "var(--wp--preset--color--background)"
        },
        "spacing": {
          "padding": {
            "bottom": "0.5rem",
            "left": "1rem",
            "right": "1rem",
            "top": "0.5rem"
          }
        }
      },
      "caption": {
        "color": {
          "text": "var(--wp--preset--color--shadow-2)"
        }
      },
      "h1": {
        "typography": {
          "fontSize": "1.75rem"
        }
      },
      "h2": {
        "typography": {
          "fontSize": "1.5rem"
        }
      },
      "h3": {
        "typography": {
          "fontSize": "1.25rem"
        }
      },
      "h4": {
        "typography": {
          "fontSize": "1.125rem"
        }
      },
      "h5": {
        "typography": {
          "fontSize": "0.83rem"
        }
      },
      "h6": {
        "typography": {
          "fontSize": "0.75rem"
        }
      },
      "heading": {
        "color": {
          "text": "var(--wp--preset--color--primary)"
        }
      },
      "link": {
        ":hover": {
          "typography": {
            "textDecoration": "none"
          }
        },
        "color": {
          "text": "var(--wp--preset--color--secondary)"
        }
      }
    },
    "spacing": {
      "blockGap": "1.0rem",
      "padding": {
        "left": "var(--wp--preset--spacing--10)",
        "right": "var(--wp--preset--spacing--10)"
      }
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--system-ui)",
      "fontSize": "14px",
      "fontStyle": "normal",
      "fontWeight": "400",
      "lineHeight": "1.4"
    }
  }
}
