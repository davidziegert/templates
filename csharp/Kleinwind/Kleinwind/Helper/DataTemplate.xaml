<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vw="clr-namespace:Kleinwind.View"
                    xmlns:vm="clr-namespace:Kleinwind.ViewModel">

    <DataTemplate DataType="{x:Type vm:EnergieViewModel}">
        <vw:EnergieView></vw:EnergieView>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:GlossarViewModel}">
        <vw:GlossarView></vw:GlossarView>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:HomeViewModel}">
        <vw:HomeView></vw:HomeView>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:InputViewModel}">
        <vw:InputView></vw:InputView>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:OutputViewModel}">
        <vw:OutputView></vw:OutputView>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:PerformanceViewModel}">
        <vw:PerformanceView></vw:PerformanceView>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:WindViewModel}">
        <vw:WindView></vw:WindView>
    </DataTemplate>
</ResourceDictionary>